{
  "parser": "babel-eslint",
  "plugins": [
    "flowtype"
  ],
  "extends": "airbnb",
  "rules": {
    "jsx-a11y/no-static-element-interactions": 0,
    "no-underscore-dangle": 1,
    "no-use-before-define": 1,
    "no-useless-escape": 1,
    "no-restricted-globals": 2,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}